{
    "name": "mrgoodbytes8667/pluralize-bundle",
    "type": "symfony-bundle",
    "description": "A (Twig) wrapper bundle for pluralizing strings via Illuminate",
    "license": "CC-BY-NC-4.0",
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-json": "*",
        "symfony/config": "^5.2 | ^6.0",
        "symfony/dependency-injection": "^5.2 | ^6.0",
        "symfony/http-kernel": "^5.2 | ^6.0",
        "symfony/string": "^5.2 | ^6.0",
        "twig/twig": "^2.12 | ^3.0"
    },
    "require-dev": {
        "doctrine/common": "^3.0",
        "symfony/error-handler": "^5.2 | ^6.0",
        "symfony/test-pack": "^1.0.9",
        "symfony/var-dumper": "^5.2 | ^6.0"
    },
    "authors": [
        {
            "name": "MrGoodBytes",
            "email": "bytes@goodbytes.live"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "conflict": {
        "phpunit/phpunit": "<9.5 || 9.5.8"
    },
    "autoload": {
        "psr-4": {
            "Bytes\\PluralizeBundle\\": "src/",
            "Bytes\\PluralizeBundle\\Tests\\": "Tests/"
        }
    }
}
